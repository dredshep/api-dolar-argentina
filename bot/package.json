    "axios": "^1.6.7",
    "dotenv": "^8.2.0",
    "grammy": "^1.21.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/sqlite3": "^3.1.11",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@types/bun": "latest"
  }
}