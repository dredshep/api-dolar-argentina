{
  "name": "api-dolar-argentina-bot",
  "version": "1.0.1",
  "description": "Telegram bot for API Dolar Argentina",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "bun run src/index.ts",
    "dev-token": "bun run --env-file=.env.development src/index.ts",
    "watch": "bun --watch src/index.ts"
